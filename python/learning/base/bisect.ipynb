   "source": [
    "import bisect\n",
    "\n",
    "a = [1,3,5,7,7,7,7,7,7,9]\n",
    "# 在有序列表a中查找元素x的插入位置 返回最左侧的索引\n",
    "bisect.bisect_left(a,7) # 3\n",
    "# 同上 但返回最右侧的索引\n",
    "bisect.bisect_right(a,7) # 9\n",
    "# 插入到左侧\n",
    "bisect.insort_left(a,6)\n",
    "# 插入到右侧\n",
    "bisect.insort_right(a,6)"
   ]
  }
